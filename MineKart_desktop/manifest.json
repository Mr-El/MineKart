{
  "name": "MineKart",
  "description": "Transfer world saves between devices!",
  "version": "0.1",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": [ "background.js" ]
    },
    "sandbox": {
      "pages": [ "index.html" ]
    }
  },
  "icons": {
    "128": "Images/mineKart_128.png",
    "16": "Images/mineKart_16.png"
  },
  //Permissions for USB devices (Android)
  "permissions": [ "usb" ]
}